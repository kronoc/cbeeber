<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:id="@+id/layout_listview_relativelayout"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
<!--
    <TextView
            android:id="@+id/layout_listview_textview"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:textSize="26sp"/>
    -->

    <ImageView
            android:id="@+id/layout_schedule_imageview"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingRight="18dp"/>

    <TextView
            android:id="@+id/layout_schedule_textview_programme_title"
            android:layout_toRightOf="@id/layout_schedule_imageview"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="26sp"
            android:textColor="#000000"/>

    <TextView
            android:id="@+id/layout_schedule_textview_programme_time"
            android:layout_toRightOf="@id/layout_schedule_imageview"
            android:layout_below="@id/layout_schedule_textview_programme_title"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="20sp"
            android:textColor="#000000"/>
<!--
    <ImageView
            android:id="@+id/activity_main_imageview"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>-->

</RelativeLayout>
